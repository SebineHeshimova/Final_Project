		<div class="layer"></div><!-- Opacity Mask Menu Mobile -->
		<div class="container-fluid">
			<div id="logo">
				<a asp-action="index" asp-controller="home">
					<img src="~/assets/img/logo.svg" width="140" height="35" alt="" class="logo_normal">
					<img src="~/assets/img/logo_sticky.svg" width="140" height="35" alt="" class="logo_sticky">
				</a>
			</div>
			<ul id="top_menu">
				<li><a href="#0" class="search-overlay-menu-btn"></a></li>
				<li>
					<div class="dropdown dropdown-cart">
						<a href="shop-cart.html" class="cart_bt"><strong>2</strong></a>
						<div class="dropdown-menu">
							<ul>
								<li>
									<figure><img src="~/assets/img/item_placeholder_square_small.jpg" data-src="~/assets/img/item_square_small_1.jpg" alt="" width="50" height="50" class="lazy"></figure>
									<strong><span>1x Pizza Napoli</span>$12.00</strong>
									<a href="#0" class="action"><i class="icon_trash_alt"></i></a>
								</li>
								<li>
									<figure><img src="~/assets/img/item_placeholder_square_small.jpg" data-src="~/assets/img/item_square_small_2.jpg" alt="" width="50" height="50" class="lazy"></figure>
